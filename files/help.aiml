<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    
    <!-- Main Help Pattern -->
    <category>
        <pattern>HELP</pattern>
        <template>
            HELP AND SUPPORT
            <br></br><br></br>
            I'm here to help you explore computer science career options! Here's what I can do:
            <br></br><br></br>
            AVAILABLE OPTIONS:
            <br></br>
            - View all 5 career paths for CS graduates
            <br></br>
            - Take a skills assessment to get personalized recommendations
            <br></br>
            - Learn detailed information about each career
            <br></br>
            - Restart your assessment at any time
            <br></br><br></br>
            COMMON COMMANDS:
            <br></br>
            - Type "START" to return to the main menu
            <br></br>
            - Type "SHOW ALL CAREERS" to browse all options
            <br></br>
            - Type "ASSESS MY SKILLS" for personalized recommendations
            <br></br>
            - Type "RESTART" to clear your responses and start over
            <br></br><br></br>
            Need additional assistance? Contact our career services team:
            <br></br>
            Phone: (555) 123-4567
            <br></br>
            Email: careerservices@university.edu
            <br></br><br></br>
            <button>
                <text>Return to Main Menu</text>
                <postback>START</postback>
            </button>
            <button>
                <text>Browse All Careers</text>
                <postback>SHOW ALL CAREERS</postback>
            </button>
            <button>
                <text>Get Recommendation</text>
                <postback>ASSESS MY SKILLS</postback>
            </button>
        </template>
    </category>
    
    <!-- Alternative Help Patterns -->
    <category>
        <pattern>HELP ME</pattern>
        <template><srai>HELP</srai></template>
    </category>
    
    <category>
        <pattern>I NEED HELP</pattern>
