    </category>
    
    <category>
        <pattern>HOW DO I USE THIS</pattern>
        <template><srai>HELP</srai></template>
    </category>
    
    <category>
        <pattern>^ HELP ^</pattern>
        <template><srai>HELP</srai></template>
    </category>
    
    <!-- Contact Information -->
    <category>
        <pattern>CONTACT</pattern>
        <template>
            CONTACT CAREER SERVICES
            <br></br><br></br>
            If you need additional assistance beyond what this chatbot provides, please reach out to our career services team:
            <br></br><br></br>
            Phone: (555) 123-4567
            <br></br>
            Email: careerservices@university.edu
            <br></br>
            Office Hours: Monday-Friday, 9:00 AM - 5:00 PM
            <br></br><br></br>
            We're here to help you succeed in your career journey!
            <br></br><br></br>
            <button>
                <text>Return to Main Menu</text>
                <postback>START</postback>
            </button>
        </template>
    </category>
    
    <category>
        <pattern>CONTACT INFO</pattern>
        <template><srai>CONTACT</srai></template>
    </category>
    
    <category>
        <pattern>CONTACT INFORMATION</pattern>
        <template><srai>CONTACT</srai></template>
    </category>
    
    <category>
        <pattern>HOW DO I CONTACT</pattern>
