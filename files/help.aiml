    </category>
    
    <!-- Goodbye/Exit -->
    <category>
        <pattern>GOODBYE</pattern>
        <template>
            Goodbye! Best of luck with your career journey in Computer Science!
            <br></br><br></br>
            If you need more guidance later, just come back and type "START" to begin again.
            <br></br><br></br>
            Remember, you can always contact our career services team at careerservices@university.edu or call (555) 123-4567.
        </template>
    </category>
    
    <category>
        <pattern>BYE</pattern>
        <template><srai>GOODBYE</srai></template>
    </category>
    
    <category>
        <pattern>SEE YOU</pattern>
